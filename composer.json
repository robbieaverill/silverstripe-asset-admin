{
	"name": "silverstripe/asset-admin",
	"description": "Asset management for the SilverStripe CMS",
	"type": "silverstripe-module",
	"require": {
		"php": ">=5.4.0",
		"silverstripe/cms": "dev-integrate-react",
		"silverstripe/framework": "dev-features/campaign-admin"
	},
	"require-dev": {
		"phpunit/PHPUnit": "^3.7"
	},
	"config": {
		"process-timeout": 600
	},
	"autoload": {
		"classmap": ["tests/behat/"]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/open-sausages/silverstripe-cms.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/open-sausages/silverstripe-framework.git"
		}
	]
}
